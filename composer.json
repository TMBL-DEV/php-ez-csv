{
    "name": "devcreations/ez-csv",
    "description": "This is my package ez-csv",
    "keywords": [
        "devcreations",
        "ez-csv"
    ],
    "homepage": "https://github.com/devcreations/ez-csv",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim",
            "email": "info@devcreations.codes",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "laravel/pint": "^1.12",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Devcreations\\EzCsv\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devcreations\\EzCsv\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
